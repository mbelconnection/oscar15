	<measurement type="PWC" typeDesc="Parents with children" typeDisplayName="Parents with children" measuringInstrc="">
		<validationRule name="Yes/No" maxValue="" minValue="" isDate="" isNumeric="" regularExp="YES|yes|Yes|Y|NO|no|No|N" maxLength="" minLength="" />
	</measurement>
	
	<measurement type="PCPR" typeDesc="Poison control prevention" typeDisplayName="Poison control prevention" measuringInstrc="">
		<validationRule name="Reiew" maxValue="" minValue="" isDate="" isNumeric="" regularExp="REVIEWED|reviewed|Reviewed" maxLength="" minLength="" />
