				<property name="providerManager">
					<ref bean="providerManager" />
				</property>
			</bean>
		</property>
		<property name="delay">
			<!-- run 3 min after start -->
			<value>180000</value>
		</property>
		<property name="period">
			<!-- run every 5 mins -->
