<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="SdmtOut" table="sdmt_out">
		<id column="recordid" name="recordId" type="integer" unsaved-value="0">
			<generator class="sequence">
   			 <param name="sequence">seq_sdmt_out</param>
			</generator>
		</id>
		<property column="batch_no" name="batchNumber"></property>
		<property column="batch_date"  name="batchDate"></property>
		<property column="first_name" name="firstName"></property>
		<property column="last_name" name="lastName"></property>
		<property column="dob" name="dob"></property> 
		<property column="sin" name="sin"></property>
		<property column="health_card_no" name="healthCardNo"></property>
		<property column="client_id" name="clientId"></property>
		<property column="sdmt_id" name="sdmtId"></property>
		<property column="sdmt_ben_unit_id" name="sdmtBenUnitId"></property>	
		<property name="sendOut" column="issent"></property>
	</class>
</hibernate-mapping>