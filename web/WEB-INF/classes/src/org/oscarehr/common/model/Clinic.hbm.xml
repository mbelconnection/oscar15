<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.oscarehr.common.model.Clinic" table="clinic">
		<id name="id" column="clinic_no" unsaved-value="0">
			<generator class="native" />
		</id>

		<property column="clinic_name" name="clinicName" not-null="false"
			type="string" />
		<property column="clinic_address" name="clinicAddress"
			not-null="false" type="string" />
		<property column="clinic_city" name="clinicCity" not-null="false"
			type="string" />
		<property column="clinic_postal" name="clinicPostal"
			not-null="false" type="string" />
		<property column="clinic_phone" name="clinicPhone" not-null="false"
			type="string" />
		<property column="clinic_fax" name="clinicFax" not-null="false"
			type="string" />
		<property column="clinic_location_code" name="clinicLocationCode"
			not-null="false" type="string" />
		<property column="status" name="status" not-null="false"
			type="string" />
		<property column="clinic_province" name="clinicProvince"
			not-null="false" type="string" />
		<property column="clinic_delim_phone" name="clinicDelimPhone"
			not-null="false" type="string" />
		<property column="clinic_delim_fax" name="clinicDelimFax"
			not-null="false" type="string" />

	</class>
</hibernate-mapping>
