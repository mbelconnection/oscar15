<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.oscarehr.common.model.FlowSheetCustomization" table="flowsheet_customization">
		<id name="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="flowsheet" />
		<property  name="measurement" />
                <property  name="action" />
		<property column="provider_no" name="providerNo" />
		<property column="demographic_no" name="demographicNo" />
		<property column="create_date" name="createDate" />
		<property name="payload" />
		<property name="archived" />
                <property column="archived_date" name="archivedDate"/>
	</class>
</hibernate-mapping>
