<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.oscarehr.consultationRequest.model.ConsultationRequest"
		table="consultationRequests">
		<id name="id" column="requestId" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="referalDate" not-null="false" type="timestamp" />
		<property name="serviceId" not-null="false" type="long" />
		<property name="specId" not-null="false" type="long" />
		<property name="appointmentDate" not-null="false" type="timestamp" />
		<property name="appointmentTime" not-null="false" type="time" />
		<property name="reason" not-null="false" type="string" />
		<property name="clinicalInfo" not-null="false" type="string" />
		<property name="currentMeds" not-null="false" type="string" />
		<property name="allergies" not-null="false" type="string" />
		<property name="providerNo" not-null="false" type="string" />
		<property name="demographicNo" not-null="false" type="long" />
		<property name="status" not-null="false" type="string" />
		<property name="statusText" not-null="false" type="string" />
		<property name="sendTo" not-null="false" type="string" />
		<property name="concurrentProblems" not-null="false" type="string" />
		<property name="urgency" not-null="false" type="string" />
		<property name="patientWillBook" not-null="false" type="long" />
	</class>
</hibernate-mapping>
