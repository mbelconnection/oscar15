<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.eyeform.model">
	<class
		name="ProcedureBook"
		table="procedurebook"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="eyeformId"
			column="eyeform_id"
			type="java.lang.Long"
			not-null="false"
		/>
		
		<property
			name="provider"
			column="provider"
			type="string"
			not-null="false"
			length="60"
		/>
		
		<property
			name="status"
			column="status"
			type="string"
		/>
		<property
			name="appointmentNo"
			column="appointment_no"
			type="integer"
			not-null="false"
		/>
		
		<property
			name="demographicNo"
			column="demographic_no"
			type="integer"
			not-null="false"			
		/>
		
		<many-to-one
			name="demographic"
			column="demographic_no"
			class="org.oscarehr.common.model.Demographic"
			not-null="false"
			insert="false" update="false"
		>
		</many-to-one>
		
		<property
			name="procedureName"
			column="procedure_name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="eye"
			column="eye"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="location"
			column="location"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="urgency"
			column="urgency"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="comment"
			column="comment"
			type="string"
			not-null="false"
			
		/>
		<property
			name="date"
			column="date"
			type="timestamp"
			not-null="false"
			length="19"
		/>


	</class>	
</hibernate-mapping>