			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="osPrism"
			column="os_prism"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="updateTime"
			column="update_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>

	</class>	
</hibernate-mapping>
