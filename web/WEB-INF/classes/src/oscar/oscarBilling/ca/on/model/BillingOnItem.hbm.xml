<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="oscar.oscarBilling.ca.on.model">
	<class name="BillingOnItem" table="billing_on_item">
		<id name="id" unsaved-value="0">
			<generator class="native" />
		</id>
                <property name="ch1_id" />
                <property name="transc_id" />
                <property name="rec_id" />
                <property name="service_code" />
                <property name="fee" />
                <property name="ser_num" />
                <property name="service_date" />
                <property name="dx" />
                <property name="dx1" />
                <property name="dx2" />
                <property name="status" />
                <property name="timestamp" />
    </class>
</hibernate-mapping>
